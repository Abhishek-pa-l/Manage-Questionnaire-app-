<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
>
    <Dialog title="Assesment Question" contentWidth="600px">
        <content>
            <f:SimpleForm
                id="simpleFormUpdated"
                editable="false"
                
            >
                <Label
                    id="demandIDLabel"
                    text="Question Category"
                />
                <Input
                    id="demandIDInput"
                    value=""
                />

                <Label
                    id="fundCenterLabel"
                    text="Question"
                />
                <Input
                    id="fundCenterInput"
                    value=""
                />

                

               
            </f:SimpleForm>
        </content>
        <Toolbar>
            <ToolbarSpacer />
            <Button
                text="Add"
                type="Emphasized"
                press="OnAddDemandDBudgetAdd"
            />
            <Button
                text="Cancel"
                type="Negative"
                press="onCancelCreateQuestionFrag"
            />
        </Toolbar>
    </Dialog>
</core:FragmentDefinition>